@model LoginResponseViewModel
@{
    ViewData["Title"] = "Login";
}

<h1>Get Refresh</h1>

<div class="form-group">
    <label class="col-form-label-lg">Access token</label>
    <p class="active"> @Model.AccessToken</p>
</div>
<div class="form-group">
    <label class="col-form-label-lg">Refresh token</label>
    <p class="active"> @Model.RefreshToken</p>
</div>
<div class="form-group">
    <label class="col-form-label-lg">Id token</label>
    <p class="active"> @Model.IdToken</p>
</div>